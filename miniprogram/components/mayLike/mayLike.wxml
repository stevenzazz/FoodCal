<view style="background:#00594f;flex-wrap:wrap;display: flex;flex-direction: row;box-sizing: border-box;">
  <view style="width:346rpx;"> 
      <view catchtap="intoProductDetail" data-id="{{item.id}}" wx:for='{{foodList}}' wx:if='{{index%2==0}}' class="productDetail">
          <image wx:if='{{item.collected_num>=50}}' class='productImage' src="https://resource.51tomi.com/images/iconv2/collectIcon.png"></image>
          <image src="{{item.thumbImage||'https://resource.51tomi.com/images/icon/product_default.png'}}" style="width:326rpx;height:326rpx;margin: 10rpx 0 0 10rpx;" mode="aspectFill"></image>
          <view class="title">{{item.name}}</view>
          <view class="tagsBox"> 
            <view class="tagGreen" wx:if='{{item.healthLight==1}}'>绿灯食品</view>
            <view class="tagYellow" wx:if='{{item.healthLight==2}}'>黄灯食品</view>
            <view class="tagRed" wx:if='{{item.healthLight==3}}'>红灯食品</view>
            <view class="{{item.healthLight==1?'tagGreen':item.healthLight==2?'tagYellow':'tagRed'}}" wx:if='{{item.calory}}'>{{'每' + item.weight + '克含' + item.calory + '卡路里'}}</view>
          </view>
      </view> 
  </view>

  <view style="width:346rpx;margin-left:18rpx;">
      <view catchtap="intoProductDetail" data-id="{{item.id}}" wx:for='{{foodList}}' wx:if='{{index%2==1}}' class="productDetail">
          <image wx:if='{{item.collected_num>=50}}' class='productImage' src="https://resource.51tomi.com/images/iconv2/collectIcon.png"></image>
          <image src="{{item.thumbImage||'https://resource.51tomi.com/images/icon/product_default.png'}}" style="width:326rpx;height:326rpx;margin: 10rpx 0 0 10rpx;" mode="aspectFill"></image>
          <view class="title">{{item.name}}</view>
          <view class="tagsBox"> 
            <view class="tagGreen" wx:if='{{item.healthLight==1}}'>绿灯食品</view>
            <view class="tagYellow" wx:if='{{item.healthLight==2}}'>黄灯食品</view>
            <view class="tagRed" wx:if='{{item.healthLight==3}}'>红灯食品</view>
            <view class="{{item.healthLight==1?'tagGreen':item.healthLight==2?'tagYellow':'tagRed'}}" wx:if='{{item.calory}}'>{{'每' + item.weight + '克含' + item.calory + '卡路里'}}</view>
          </view>
      </view> 
  </view>


